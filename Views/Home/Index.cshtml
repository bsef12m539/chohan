                            <h3 id="feat-head" class="big_title">
                                Featured Properties              </small>
                            </h3>
                        </div>
                        @if (ViewBag.prop != null)
                        {
                            foreach (property p in ViewBag.prop)
                            {
                                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">

                                    <div class="boxes first featured_prop_adj" data-effect="slide-bottom">
                                        <div class="ImageWrapper boxes_img">

                                            <table align="center" class="featured-image-table" cellpadding="0" cellspacing="0">
                                                <tr>
                                                    <td>
                                                        <img src="@p.pimage" alt="">
                                                    </td>
                                                </tr>
                                            </table>
                                            <!---PRICE-->
                                            <div class="box_type">PKR @p.price</div>
                                            <div class="status_type">@p.type</div>
                                        </div>
                                        <h2 class="title">
                                            <a href="#">@p.Description</a>
                                            <small class="small_title">
                                               @p.pAddress
                                            </small>
                                        </h2>
                                        <div class="boxed_mini_details1 clearfix">

                                            <span class="sqft last">

                                                <strong>Area</strong>
                                                <i class="bg-ico area-ico"></i> @p.Area @p.Unit
                                            </span>


                                        </div>
                                    </div>
                                    <!-- end boxes -->
                                </div>
                            }

                        }
                             
                        
                        
